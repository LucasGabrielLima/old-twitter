{
    "manifest_version": 2,
    "name": "Old Twitter Design",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*", "http://twitter.com/*", "https://*.twitter.com/*"],
            "js": ["content.js"],
            "css": ["style.css"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png"
    }
}
